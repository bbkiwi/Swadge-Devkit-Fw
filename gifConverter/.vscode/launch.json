    "version": "0.2.0",
    "configurations": [
        {
            "name": "lzCompressor Launch Gallery",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/lzCompressor",
            "args": [
                "bins/bongo.bin",
                "bins/funkus.bin",
                "bins/gaylord.bin",
                "bins/logo.bin",
                "bins/snort.bin",
                "bins/unlock_maze.bin",
                "bins/unlock_tiltrads.bin",
                "bins/unlock_snake.bin",
                "bins/unlock_joust.bin"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "lzCompressor Launch Gallery2",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/lzCompressor",
